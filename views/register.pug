doctype html
html(lang='zh-CN')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible',content='IE=edge')
    meta(name='viewport',content='width=device-width, initial-scale=1')
    meta(name='description',content='')
    meta(name='author',content='')
    title 注册
    link(rel='stylesheet',href='//cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet',href='//cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap-theme.min.css')
    link(rel='stylesheet',href='//cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/css/bootstrap-dialog.min.css')
    link(rel='stylesheet',href='/sb-admin-2/sb-admin-2.css')
    link(rel='stylesheet',href='/stylesheets/style.css')
  body
    div.container
      div.row
        div.col-md-4.col-md-offset-4
          div.login-panel.panel.panel-default
            div.panel-heading
              h3.panel-title 注册新用户
            div.panel-body
              form#registerForm(role='form')
                fieldset
                  div.form-group
                    input#telephone.form-control(placeholder="手机号",name="telephone",type="text",autofocus )
                  div.form-group
                    input.form-control(placeholder="密码",name="password",type="password" )
                  input.btn.btn-lg.btn-success.btn-block(type='submit',value='注册') 
    
    script(src='//cdn.bootcss.com/jquery/2.2.4/jquery.min.js')
    script(src='//cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min.js')
    script(src='//cdn.bootcss.com/jquery-validate/1.16.0/jquery.validate.min.js')
    script(src='/javascript/jquery.validate.extend.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/js/bootstrap-dialog.min.js')
    script(src='/javascript/web.js')
    script(src='/javascript/register.js')